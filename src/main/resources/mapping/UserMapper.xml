<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mkd.adtools.mapper.UserMapper">
	<select id="selectSysUser" resultType="AdSysUser">
		select * from ad_sys_user where username=#{username} and password=#{password} limit 0,1
	</select>
</mapper>